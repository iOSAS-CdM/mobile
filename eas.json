{
	"cli": {
		"version": ">= 10.0.0"
	},
	"build": {
		"debug": {
			"distribution": "internal",
			"android": {
				"buildType": "apk",
				"gradleCommand": ":app:assembleDebug"
			},
			"env": {
				"EXPO_PUBLIC_ENV": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"android": {
				"buildType": "app-bundle"
			}
		}
	}
}
